<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XFMCSample.Views.ThirdPage">
  <StackLayout>
    <Entry x:Name="UrlText" Text="http://www.xamarin.com"/>
    <WebView BindingContext="{x:Reference Name=UrlText}"
           Source="{Binding Text}" WidthRequest="200" HeightRequest="1000"/>
  </StackLayout>
</ContentPage>